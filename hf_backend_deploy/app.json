{
  "name": "Efficio Todo Hub",
  "description": "AI-powered todo management application with natural language processing",
  "repository": "https://github.com/your-username/efficio-todo-hub",
  "logo": "",
  "keywords": [
    "fastapi",
    "todo",
    "ai",
    "productivity",
    "nlp"
  ],
  "success_url": "/",
  "env": {
    "SECRET_KEY": {
      "description": "JWT secret key for authentication",
      "required": true
    },
    "DATABASE_URL": {
      "description": "PostgreSQL database URL",
      "required": true
    },
    "FRONTEND_URL": {
      "description": "Frontend application URL",
      "required": false,
      "value": "https://your-frontend-url.com"
    }
  },
  "scripts": {
    "postdeploy": "python db_init.py"
  }
}